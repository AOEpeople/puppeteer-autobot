{
  "name": "fetchbot",
  "version": "1.2.0-alpha",
  "description": "A library and shell command that provides a simple JSON-API to perform human like interactions and data extractions on any website. Built on top of pupeteerr.",
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "dist/index.js",
  "author": "Bernhard Behrendt <bernhard.behrendt@aoe.com>",
  "license": "GPL-3.0./bi ",
  "repository": {
    "type": "git",
    "url": "https://github.com/AOEpeople/puppeteer-fetchbot"
  },
  "bin": {
    "fetchbot": "bin/fetchbot"
  },
  "dependencies": {
    "@types/chai": "^4.1.2",
    "@types/command-line-args": "^4.0.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "@types/puppeteer": "^1.0.0",
    "@types/winston": "^2.3.8",
    "ansi-escape-sequences": "^4.0.0",
    "chai": "^4.1.2",
    "command-line-args": "^5.0.1",
    "command-line-usage": "^4.1.0",
    "coveralls": "^3.0.0",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "puppeteer": "^1.0.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {},
  "scripts": {
    "postinstall": "npm run build",
    "build": "./node_modules/.bin/tsc",
    "test": "NODE_ENV=testing ./node_modules/.bin/mocha -r ts-node/register ./src/lib/**/*.spec.ts",
    "cover": "./node_modules/.bin/nyc npm run test",
    "coveralls": "npm run cover && ./node_modules/.bin/coveralls < coverage/lcov.info",
    "cli": "node ./dist/cli.js",
    "cli:dev": "./node_modules/.bin/ts-node ./src/cli.ts"
  },
  "nyc": {
    "include": [
      "*.ts",
      "**/*.ts"
    ],
    "exclude": [
      "./",
      "*.spec.ts",
      "**/*.spec.ts",
      "src/lib/classes/fetcher.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov"
    ],
    "all": true
  }
}
